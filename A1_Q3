#include <iostream>
using namespace std; 

int main() {
    int n;
    cout<<"enter the number: ";
    cin>>n;
    bool flag=true;
    
    for(int i=2;i<n;i++){
        if(n%i==0){
            //cout<<"NOT A PRIME NUMEBER"<<endl;
            flag=false;
            
            break;
            
        }
        
    }
    if(flag){
        cout<<"PRIME";
    }
    else{
        cout<<"NOT PRIME";
        
    }
    return 0;
}
